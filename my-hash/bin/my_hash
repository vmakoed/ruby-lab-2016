$LOAD_PATH.unshift "#{File.dirname(__FILE__)}/../lib"

require 'my_hash'

hash_object = MyHash.new

hash_object[:apple] = 5
hash_object[:orange] = 3
puts hash_object[:apple]
puts hash_object[:orange]

puts hash_object.keys
puts hash_object.values
puts hash_object.size

another_hash_object = MyHash.new
another_hash_object[:apple] = 5
another_hash_object[:orange] = 3
puts hash_object.eql?(another_hash_object)
another_hash_object[:orange] = 2
puts hash_object.eql?(another_hash_object)

